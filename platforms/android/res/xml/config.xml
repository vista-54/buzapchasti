<?xml version='1.0' encoding='utf-8'?>
<widget id="com.vista.Buzapchasti" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <name>Buzapchasti</name>
    <description>
        Приложение для покупки автомобильных запчастей
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Vista
    </author>
    <content src="index.html" />
    <access origin="*" />
    <gap:plugin name="com.ionic.keyboard" version="1.0.3" />
    <gap:plugin name="net.yoik.cordova.plugins.screenorientation" version="1.3.1" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="phonegap-version" value="3.6.3" />
    <preference name="orientation" value="default" />
    <preference name="permissions" value="none" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
</widget>
